{% if object.children is defined and object.children|length == 0 or object.items is defined and object.items|length == 0 %}
    <span class="btn btn-default btn-sm">Cet élément n'a aucun sous-item</span>
{% else %}
    <a class="btn btn-primary btn-sm" href="{{ admin.generateObjectUrl('item', object) }}">Gérér les items</a>
{% endif %}
